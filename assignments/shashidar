var employees=[]
 employees[0]={name:"shashi", age:32, dob:"March 12, 2014"}
 employees[1]={name:"ramesh", age:17, dob:"June 2, 2023"}
 employees[2]={name:"Chandu", age:58, dob:"December 20, 2036"}
