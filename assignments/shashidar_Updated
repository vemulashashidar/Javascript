<html>
    <body>
    <table>
            
                    <tr>
                        <th>NAME</th>
                        <th>AGE</th>
                        <th>DOB</th>
                    </tr>
                    <tbody id="tableData"></tbody>
                </table>
    </body>
<script type="text/javascript">
    //we can ask size and add for loop for taking values from user if needed
var employees=[]
 employees[0]={name:"chandu", age:32, DOB:"March 12, 2014"}
 employees[1]={name:"shashi", age:17, DOB:"June 2, 2023"}
 employees[2]={name:"bharath", age:58, DOB:"December 20, 2036"}
 employees[3]={name:"rajesh", age:62, DOB:"April 30, 2020"}
    
    var t='<body>';
    for(i=0;i<employees.length;i++){
               t+= '<tr>';
                        t+= '<td>' + employees[i].name   +'</td>';
                        t+= '<td>' + employees[i].age + '</td>';
                        t+= '<td>' + employees[i].DOB  + '</td>';
                        t+= '</tr>';
  }
    t += '<body>';
    document.getElementById('tableData').innerHTML = t;
     </script>
    </html>
